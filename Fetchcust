

// Directly setting values in queuemapper without initialization or null checks
queuemapper.setCurrStatusCode(record.getInt("QUSTATUSCODE"));
queuemapper.setCurrStatusDesc(record.getString("QUSTATUSDESC"));
queuemapper.setMndStatusCode(record.getString("MNDSTATUSCODE"));
queuemapper.setMndStatusDesc(record.getString("MNDSTATUSDESC"));
queuemapper.setRespCode(record.getString("RESPCODE"));
queuemapper.setErrorCode(record.getString("ERRORCODE"));
