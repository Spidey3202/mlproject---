
// Initialize all Bin variables as null
Bin txnId = null;
Bin messageType = null;
Bin upiId = null;
Bin queueStatus = null;
Bin queueStatusDesc = null;
Bin queueRefNo = null;

// Create a collection to hold the Bin objects
List<Bin> columns = new ArrayList<>();

// Fetch values from queueMessage and check if not blank before setting
if (StringUtils.isNotBlank(queueMessage.getTxnId())) {
    txnId = new Bin("TXNID", queueMessage.getTxnId());
    columns.add(txnId);
    addRDto.setTxnId(queueMessage.getTxnId());
}

if (StringUtils.isNotBlank(queueMessage.getMessageType())) {
    messageType = new Bin("MESSAGETYPE", queueMessage.getMessageType());
    columns.add(messageType);
    addRDto.setMessageType(queueMessage.getMessageType());
}

if (StringUtils.isNotBlank(queueMessage.getUpiId())) {
    upiId = new Bin("UPIID", queueMessage.getUpiId());
    columns.add(upiId);
    addRDto.setUpiId(queueMessage.getUpiId());
}

if (StringUtils.isNotBlank(queueMessage.getQueueStatus())) {
    queueStatus = new Bin("QUEUESTATUS", queueMessage.getQueueStatus());
    columns.add(queueStatus);
    addRDto.setQueueStatus(queueMessage.getQueueStatus());
}

if (StringUtils.isNotBlank(queueMessage.getQueueStatusDesc())) {
    queueStatusDesc = new Bin("QUEUESTATUSDESC", queueMessage.getQueueStatusDesc());
    columns.add(queueStatusDesc);
    addRDto.setQueueStatusDesc(queueMessage.getQueueStatusDesc());
}

if (StringUtils.isNotBlank(queueMessage.getQueueRefNo())) {
    queueRefNo = new Bin("QUEUEREFNO", queueMessage.getQueueRefNo());
    columns.add(queueRefNo);
    addRDto.setQueueRefNo(queueMessage.getQueueRefNo());
}
