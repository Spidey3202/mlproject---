

Bin txnId = null;
Bin msgTypeId = null;
Bin fvaName = null;
Bin fvaAddr = null;
Bin quStatusCode = null;
Bin quStatusDesc = null;
Bin trnRefNo = null;
Bin txnType = null;
Bin resMsg = null;
Bin txnInit = null;
Bin respCode = null;
Bin approvalNum = null;
Bin txnAmount = null;
Bin prfVAddr = null;
Bin errorCode = null;
Bin pyRespCode = null;

List<Bin> columns = new ArrayList<>();

if (StringUtils.isNotBlank(queue.getTxnId())) {
    txnId = new Bin("TXNID", queue.getTxnId());
    columns.add(txnId);
}
if (StringUtils.isNotBlank(queue.getMsgTypeId())) {
    msgTypeId = new Bin("MSGTYPEID", queue.getMsgTypeId());
    columns.add(msgTypeId);
}
if (StringUtils.isNotBlank(queue.getFvaName())) {
    fvaName = new Bin("FVANAME", queue.getFvaName());
    columns.add(fvaName);
}
if (StringUtils.isNotBlank(queue.getFvaAddr())) {
    fvaAddr = new Bin("FVADDR", queue.getFvaAddr());
    columns.add(fvaAddr);
}
if (queue.getQuStatusCode() != null) {
    quStatusCode = new Bin("CURRSTATUSCODE", queue.getQuStatusCode());
    columns.add(quStatusCode);
}
if (StringUtils.isNotBlank(queue.getQuStatusDesc())) {
    quStatusDesc = new Bin("CURRSTATUSDESC", queue.getQuStatusDesc());
    columns.add(quStatusDesc);
}
if (StringUtils.isNotBlank(queue.getTrnRefNo())) {
    trnRefNo = new Bin("TRNREFNO", queue.getTrnRefNo());
    columns.add(trnRefNo);
}
if (StringUtils.isNotBlank(queue.getTxnType())) {
    txnType = new Bin("TXNTYPE", queue.getTxnType());
    columns.add(txnType);
}
if (StringUtils.isNotBlank(queue.getResMsg())) {
    resMsg = new Bin("RESMSG", queue.getResMsg());
    columns.add(resMsg);
}
if (StringUtils.isNotBlank(queue.getTxnInit())) {
    txnInit = new Bin("TXNINIT", queue.getTxnInit());
    columns.add(txnInit);
}
if (StringUtils.isNotBlank(queue.getRespCode())) {
    respCode = new Bin("RESPCODE", queue.getRespCode());
    columns.add(respCode);
}
if (StringUtils.isNotBlank(queue.getApprovalNum())) {
    approvalNum = new Bin("APPROVALNUM", queue.getApprovalNum());
    columns.add(approvalNum);
}
if (StringUtils.isNotBlank(queue.getTxnAmount())) {
    txnAmount = new Bin("TXNAMOUNT", queue.getTxnAmount());
    columns.add(txnAmount);
}
if (StringUtils.isNotBlank(queue.getPrfVAddr())) {
    prfVAddr = new Bin("PRFVADDR", queue.getPrfVAddr());
    columns.add(prfVAddr);
}
if (StringUtils.isNotBlank(queue.getErrorCode())) {
    errorCode = new Bin("ERRORCODE", queue.getErrorCode());
    columns.add(errorCode);
}
if (StringUtils.isNotBlank(queue.getPyRespCode())) {
    pyRespCode = new Bin("PYRESPCODE", queue.getPyRespCode());
    columns.add(pyRespCode);
}
